{
  "name": "@buggyorg/library-client",
  "version": "0.1.0",
  "description": "Client implementation for Buggy library access.",
  "main": "./lib/client.js",
  "scripts": {
    "test": "standard src/**/*.js && mocha --compilers js:babel-register",
    "build": "babel src -d lib",
    "prepublish": "babel src -d lib",
    "doc": "jsdoc -c .jsdoc.json",
    "publish-docs": "jsdoc -c .jsdoc.json && git subtree split --prefix jsdoc -b gh-pages && git push -f origin gh-pages:gh-pages && git branch -D gh-pages"
  },
  "author": "Maximilian Klein",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-http": "^3.0.0",
    "mocha": "^2.4.5",
    "standard": "^6.0.8"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "request-promise-native": "^1.0.2",
    "portfinder": "^1.0.5",
    "url-join": "^1.1.0"
  }
}
